        "dead8": "animation.dead8",
        "dead9": "animation.dead9",
        "dead10": "animation.dead10",
        "dead11": "animation.dead11",
        "dead12": "animation.dead12",
        "dead13": "animation.dead13",
        "dead14": "animation.dead14",
